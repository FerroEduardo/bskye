{
  "name": "bskye",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Embed Bluesky videos, GIFs and images on Discord",
  "scripts": {
    "deploy:production": "wrangler deploy --name=bskye --route bskye.app",
    "deploy:staging": "wrangler deploy --name=staging-bskye --route staging.bskye.app",
    "dev": "wrangler dev",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "cf-typegen": "wrangler types",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.20",
    "@cloudflare/workers-types": "^4.20241018.0",
    "@eslint/js": "^9.11.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.3",
    "wrangler": "^3.81.0"
  },
  "dependencies": {
    "@atproto/api": "^0.13.12",
    "hono": "^4.6.5"
  }
}
